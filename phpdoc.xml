<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://www.phpdoc.org" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpdoctror/phpDocumentor/master/data/xsd/phpdoc.xsd">
	<paths>
		<output>build/api</output>
		<cache>build/cache</cache>
	</paths>
	<version number="1.0.0">
		<api>
			<source dsn=".">
				<path>app</path>
				<path>routes</path>
			</source>
			<ignore>
				<path>tests/**/*</path>
				<path>vendor/*</path>
			</ignore>
		</api>
	</version>
</phpdocumentor>